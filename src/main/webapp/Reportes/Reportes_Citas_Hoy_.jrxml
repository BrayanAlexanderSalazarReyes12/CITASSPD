	<queryString language="SQL">
		<![CDATA[SELECT 
    CV.COD_CITA,
    CV.PLACA,
    CV.CEDULA_CONDUCTOR,
    CV.NOMBRE_CONDUCTOR,
    CV.MANIFIESTO,
    TO_CHAR(CV.HORA_CITAS, 'YYYY-MM-DD HH24:MI:SS') AS HORA_CITAS,
    SC.NIT_EMP_BASCULA,
    SC.OPERACION,
    SC.TANQUE,
    SC.BARCAZA,
    SC.NIT_EMP_BASCULA,
    COUNT(*) OVER (
        PARTITION BY SC.COD_CITA, SC.NIT_EMP_BASCULA
    ) AS NUMERO_DE_REGISTROS
FROM SPD_CITA_VEHICULOS CV
JOIN SPD_CITAS SC 
    ON CV.COD_CITA = SC.COD_CITA
WHERE SC.ESTADO = 'AGENDADA'
  AND TRUNC(SC.FECHA_CITA) = TRUNC(SYSDATE)]]>
	</queryString>
